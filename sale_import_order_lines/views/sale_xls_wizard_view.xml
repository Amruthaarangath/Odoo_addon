<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <record id="view_sale_order_form" model="ir.ui.view">
       <field name="name">sale.view.form.inherit.sale.xls.wizard.redirect</field>
       <field name="model">sale.order</field>
       <field name="inherit_id" ref="sale.view_order_form"/>
       <field name="arch" type="xml">
           <xpath expr="//header"
                  position="inside">
                   <button id="btn" name="action_import_file" string="Import Lines" type="object"/>
           </xpath>
       </field>
   </record>
</odoo>
